{
    "name": "vasilgerginski/filamentphp-text-extractor",
    "description": "Extract and manage translatable text from Filament models with an intuitive admin interface",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "translation",
        "text-extraction",
        "i18n",
        "localization"
    ],
    "authors": [
        {
            "name": "Vasil Gerginski",
            "email": "vasil.gerginsky@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "filament/filament": "^3.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "VasilGerginski\\FilamentTextExtractor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VasilGerginski\\FilamentTextExtractor\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "VasilGerginski\\FilamentTextExtractor\\FilamentTextExtractorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}